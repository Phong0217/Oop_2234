                  <String fx:value="text-navi-bar" />
                  <String fx:value="button-history" />
               </styleClass>
            </Button>
         </children>
      </VBox>
      <VBox layoutY="625.0" prefHeight="49.0" prefWidth="72.0" styleClass="button-setting">
         <children>
            <Button contentDisplay="TOP" mnemonicParsing="false" prefHeight="49.0" prefWidth="72.0" text="Setting">
               <styleClass>
                  <String fx:value="button-setting" />
